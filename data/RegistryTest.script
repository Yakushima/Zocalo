CREATE MEMORY TABLE CLAIMS(CLAIMS_ID BIGINT NOT NULL PRIMARY KEY,CLAIM_TYPE VARCHAR(255) NOT NULL,NAME VARCHAR(255),OWNER BIGINT NOT NULL,DESCRIPTION VARCHAR(255))
CREATE MEMORY TABLE COUPONBANKS(COUPONBANK_ID BIGINT NOT NULL PRIMARY KEY,MINTED NUMERIC,REDEEMED NUMERIC,FUNDS_ID BIGINT NOT NULL,CLAIM BIGINT NOT NULL,CONSTRAINT SYS_CT_3070 UNIQUE(FUNDS_ID))
